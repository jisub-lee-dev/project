{
  "name": "project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,md,json}\"",
    "db:generate": "turbo run db:generate",
    "db:push": "turbo run db:push",
    "db:studio": "turbo run db:studio",
    "db:migrate": "turbo run db:migrate",
    "setup": "bash scripts/setup.sh",
    "health-check": "bash scripts/health-check.sh",
    "check-all": "pnpm lint && pnpm type-check && pnpm format:check",
    "fix-all": "pnpm lint:fix && pnpm format",
    "reset": "turbo run clean && rm -rf node_modules && pnpm install",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "docker:build": "docker build -t project .",
    "docker:run": "docker run -p 3000:3000 project",
    "docker:dev": "docker-compose up -d postgres",
    "docker:down": "docker-compose down",
    "update-deps": "pnpm update --recursive",
    "precommit": "pnpm check-all"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/eslint": "^8.56.0",
    "@types/jest": "^29.5.8",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.0",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "turbo": "^2.5.4",
    "typescript": "^5.6.0"
  },
  "packageManager": "pnpm@10.12.4",
  "pnpm": {
    "overrides": {
      "lucide-react>react": "^19.0.0"
    },
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "@tailwindcss/oxide",
      "prisma",
      "sharp",
      "unrs-resolver"
    ]
  }
}
